CONTAINER Xsdkgradient
{
	NAME Xsdkgradient;

	INCLUDE Mpreview;
	INCLUDE Xbase;

	GROUP ID_SHADERPROPERTIES
	{
		GRADIENT SDKGRADIENTSHADER_COLOR { COLOR; ICC_BASEDOCUMENT; }
		LONG	SDKGRADIENTSHADER_MODE
		{
			CYCLE
			{
				SDKGRADIENTSHADER_MODE_U~200000135;
				SDKGRADIENTSHADER_MODE_V~200000136;
				SDKGRADIENTSHADER_MODE_DIAGONAL~200000137;
				SDKGRADIENTSHADER_MODE_RADIAL~200000138;
				SDKGRADIENTSHADER_MODE_CIRCULAR~200000139;
				SDKGRADIENTSHADER_MODE_BOX~200000140;
				SDKGRADIENTSHADER_MODE_STAR~200000141;
				SDKGRADIENTSHADER_MODE_CORNER~200000142;
			}
		}
